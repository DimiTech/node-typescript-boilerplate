{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "semicolon": [ true, "never" ],
      "no-console": {
        "severity": "warn"
      },
      "quotemark": [ true, "single", "avoid-escape" ]
    },
    "rulesDirectory": []
}
